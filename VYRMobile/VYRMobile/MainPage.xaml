             mc:Ignorable="d"
             xmlns:local="clr-namespace:VYRMobile"
             x:Class="VYRMobile.MainPage"
             NavigationPage.HasNavigationBar="False">

    <Grid>
        <Label
            x:Name="labelCaption"
